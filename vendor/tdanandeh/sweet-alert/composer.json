{
  "name": "tdanandeh/sweet-alert",
  "description": "A simple PHP package to show Sweet Alerts with the Laravel Framework",
  "keywords": [
    "laravel",
    "alert",
    "sweet",
    "notifier"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tohid Danandeh",
      "email": "tdanandeh@yahoo.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0",
    "illuminate/session": "~5.0|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4.0",
    "friendsofphp/php-cs-fixer": "^3.9.0"
  },
  "autoload": {
    "psr-4": {
      "Tdanandeh\\SweetAlert\\": "src/SweetAlert/"
    },
    "files": [
      "src/SweetAlert/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tdanandeh\\SweetAlert\\SweetAlertServiceProvider"
      ],
      "aliases": {
        "Alert": "Tdanandeh\\SweetAlert\\SweetAlert"
      }
    }
  }
}
